<div class="advacheck {{typeid}}">
    {{{loader}}}
    <div class="advacheck-data {{typeid}}">
        <span class="courseid">{{courseid}}</span>
        <span class="doctype">{{doctype}}</span>
        <span class="content">{{content}}</span>
        <span class="userid">{{userid}}</span>
        <span class="typeid">{{typeid}}</span>
    </div>    
    <a class="advacheck-checkbtn {{typeid}}" title="{{#str}}check_advacheck, plagiarism_advacheck{{/str}}" href="#">
        <i class="fa-solid fa-circle-play fa-2x"></i>
    </a>
</div>
{{{plagiarismresult}}}
<span class="advacheck_help {{typeid}}">
    {{{advacheck_help}}}
</span>